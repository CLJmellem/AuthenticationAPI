{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbSettings": {
    "ConnectionString": "Your-MongoDB-ConnectionString",
    "DatabaseName": "Your-DatabaseName"
  },
  "Jwt": {
    "Key": "Your-Jwt-Secret-Key", // Need to be at least 64 characters for HMACSHA512
    "Issuer": "Your-Issuer",
    "Audience": "Your-Audience",
    "Expiration": "Your-Expiration" // in minutes
  },
  "Encryption": {
    "Key": "Your-Encryption-Key"
  }
}